---
apiVersion: v1
kind: Pod
metadata:
  labels:
    run: demo-exceed-max-mem
    app.kubernetes.io/name: demo-exceed-max-mem
  name: demo-exceed-max-mem
  namespace: {{ tanzu_workload_tenancy.demo_namespace }}
spec:
  containers:
    - image: projects.registry.vmware.com/rpk/nginx:1.19.5-debian-10-r8
      name: demo-exceed-max-mem
      resources:
        requests:
          memory: 6Gi
          cpu: 500m
        limits:
          memory: 6Gi
          cpu: 500m
      securityContext:
        runAsUser: 101
  dnsPolicy: ClusterFirst
  restartPolicy: Always
